    <types>
        <schema targetNamespace="http://workflow.xinco.bluecubs.com" 
                xmlns="http://www.w3.org/2001/XMLSchema">
            <import namespace="http://xml.apache.org/xml-soap"/>
            <import namespace="http://schemas.xmlsoap.org/soap/encoding/"/>
            <complexType name="Activity">
                <sequence>
                    <element name="id" type="xsd:int"/>
                    <element name="description" type="xsd:string"/>
                    <element name="assignedTo" type="tns:Resource"/>
                    <element name="assignedOn" type="xsd:dateTime"/>
                    <element name="completedOn" type="xsd:dateTime"/>
                    <element name="properties" nillable="true" type="apachesoap:Vector"/>
                </sequence>
            </complexType>
            <complexType name="Resource">
                <sequence>
                    <element name="change" type="xsd:boolean"/>
                    <element name="changerID" type="xsd:int"/>
                    <element name="email" nillable="true" type="xsd:string"/>
                    <element name="firstname" nillable="true" type="xsd:string"/>
                    <element name="id" type="xsd:int"/>
                    <element name="name" nillable="true" type="xsd:string"/>
                    <element name="reason" nillable="true" type="xsd:string"/>
                    <element name="status_number" type="xsd:int"/>
                    <element name="username" nillable="true" type="xsd:string"/>
                    <element name="userpassword" nillable="true" type="xsd:string"/>
                    <element name="writeGroups" type="xsd:boolean"/>
                    <element name="xinco_core_groups" nillable="true" type="apachesoap:Vector"/>
                </sequence>
            </complexType>
            <complexType name="Property">
                <sequence>
                    <element name="id" type="xsd:int"/>
                    <element name="transactionId" type="xsd:int" nillable="true"/>
                    <element name="nodeId" type="xsd:int" nillable="true"/>
                    <element name="activityId" type="xsd:int" nillable="true"/>
                    <element name="description" type="xsd:string"/>
                    <element name="stringProperty" type="xsd:string" nillable="true"/>
                    <element name="intProperty" type="xsd:int"/>
                    <element name="longProperty" type="xsd:long"/>
                    <element name="boolProperty" type="xsd:boolean"/>
                </sequence>
            </complexType>
            <complexType name="Node">
                <sequence>
                    <element name="id" type="xsd:int"/>
                    <element name="description" type="xsd:string"/>
                    <element name="properties" nillable="true" type="apachesoap:Vector"/>
                    <element name="activities" nillable="true" type="apachesoap:Vector"/>
                </sequence>
            </complexType>
            <complexType name="Transaction">
                <sequence>
                    <element name="id" type="xsd:int"/>
                    <element name="description" type="xsd:string"/>
                    <element name="from" type="tns:Node"/>
                    <element name="to" type="tns:Node"/>
                    <element name="activities" nillable="true" type="apachesoap:Vector"/>
                    <element name="properties" nillable="true" type="apachesoap:Vector"/>
                </sequence>
            </complexType>
            <complexType name="WorkflowTemplate">
                <sequence>
                    <element name="id" type="xsd:int"/>
                    <element name="description" type="xsd:string"/>
                    <element name="creationTime" type="xsd:dateTime"/>
                    <element name="nodes" nillable="true" type="apachesoap:Vector"/>
                    <element name="transactions" nillable="true" type="apachesoap:Vector"/>
                </sequence>
            </complexType>
            <complexType name="WorkflowInstance">
                <sequence>
                    <element name="id" type="xsd:int"/>
                    <element name="templateId" type="xsd:int"/>
                    <element name="creationTime" type="xsd:dateTime"/>
                    <element name="nodes" nillable="true" type="apachesoap:Vector"/>
                    <element name="transactions" nillable="true" type="apachesoap:Vector"/>
                </sequence>
            </complexType>
            <complexType name="WorkflowVersion">
                <sequence>
                    <element name="version_high" type="xsd:int"/>
                    <element name="version_low" type="xsd:int"/>
                    <element name="version_mid" type="xsd:int"/>
                    <element name="version_postfix" nillable="true" type="xsd:string"/>
                </sequence>
            </complexType>
            <complexType name="WorkflowSetting">
                <sequence>
                    <element name="id" type="xsd:int"/>
                    <element name="description" type="xsd:string"/>
                    <element name="int_value" type="xsd:int"/>
                    <element name="string_value" type="xsd:string"/>
                    <element name="bool_value" type="xsd:boolean"/>
                    <element name="changerID" type="xsd:int"/>
                    <element name="long_value" type="xsd:long"/>
                    <element name="workflow_settings" nillable="true" type="apachesoap:Vector"/>
                </sequence>
            </complexType>
        </schema>
        <schema targetNamespace="http://xml.apache.org/xml-soap" 
                xmlns="http://www.w3.org/2001/XMLSchema">
            <import namespace="http://workflow.xinco.bluecubs.com"/>
            <import namespace="http://schemas.xmlsoap.org/soap/encoding/"/>
            <complexType name="Vector">
                <sequence>
                    <element maxOccurs="unbounded" minOccurs="0" name="item" type="xsd:anyType"/>
                </sequence>
            </complexType>X
        </schema>
    </types>
