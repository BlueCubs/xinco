<?xml version='1.0' encoding='UTF-8'?>


<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config>
    <managed-bean>
        <managed-bean-name>XincoSessionBean</managed-bean-name>
        <managed-bean-class>com.bluecubs.xinco.web.XincoSessionBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>XincoSettingManager</managed-bean-name>
        <managed-bean-class>com.bluecubs.xinco.web.XincoSettingManager</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>XincoApplicationBean</managed-bean-name>
        <managed-bean-class>com.bluecubs.xinco.web.XincoApplicationBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>XincoRequestBean</managed-bean-name>
        <managed-bean-class>com.bluecubs.xinco.web.XincoRequestBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>XincoAdminHeader</managed-bean-name>
        <managed-bean-class>com.bluecubs.xinco.web.XincoAdminHeader</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
